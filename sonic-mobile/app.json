{
  "expo": {
    "name": "Sonic Mobile",
    "slug": "sonic-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "sonicmobile",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#0c0c0f",
    "primaryColor": "#c0162b",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Camera access is required to participate in Daily video sessions.",
        "NSMicrophoneUsageDescription": "Microphone access is required for audio in Daily calls."
      }
    },
    "android": {
      "permissions": ["CAMERA", "RECORD_AUDIO"],
      "package": "com.sonic.mobile"
    },
  "plugins": [],
    "extra": {
      "dailyApiBase": "https://api.daily.co/v1"
    }
  }
}
